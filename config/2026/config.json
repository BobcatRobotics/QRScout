                    "formResetBehavior": "reset",
                    "defaultValue": "No",
                    "choices": {
                        "Yes": "Yes",
                        "No": "No",
                        "NA": "Not Applicable"
                    }
                },
                {
                    "title": "Defense Effectiveness",
                    "description": "Rate the robot's defense effectiveness.",
                    "type": "range",
                    "required": false,
                    "code": "defenceEffe",
                    "formResetBehavior": "reset",
                    "defaultValue": 3,
                    "min": 0,
                    "max": 5,
                    "step": 1
                }
            ]
        },
